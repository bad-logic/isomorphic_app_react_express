 {
    "compilerOptions": {
      "outDir": "./dist/server",
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react-jsx",
      "skipLibCheck": true,
      "allowSyntheticDefaultImports" : true,
      "esModuleInterop" : true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "moduleResolution": "node",
    },
    "include": [
      "src"
    ],
    "exclude": ["node_modules"]
  }
  